<?xml version="1.0" encoding="UTF-8"?>
<application xmlns="http://java.sun.com/xml/ns/j2ee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	version="1.4" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/application_1_4.xsd">

	<display-name>pslweb</display-name>

	
  	<!-- web module relativo al web-service con codice=be e binding=rest --> 
  	<module>
		<!-- front office -->
    	<web>
      		<web-uri>appl-be-rest.war</web-uri>
     		<context-root>pslweb</context-root>
    	</web>
  	</module>
  	<module>
		<!-- front office fascicolo cittadino -->
    	<web>
      		<web-uri>applfc-be-rest.war</web-uri>
     		<context-root>pslfcweb</context-root>
    	</web>
  	</module>
  	<module>
		<!-- back office -->
    	<web>
      		<web-uri>applbo-be-rest.war</web-uri>
     		<context-root>pslboweb</context-root>
    	</web>
  	</module>
  	<module>
		<!-- back office -->
    	<web>
      		<web-uri>applhome-be-rest.war</web-uri>
     		<context-root>pslphome</context-root>
    	</web>
  	</module>
 
  	<module>
		<!-- mappa -->
    	<web>
      		<web-uri>applpublic-be-rest.war</web-uri>
     		<context-root>pslpublicweb</context-root>
    	</web>
  	</module>

</application>
 